{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Highlights",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "category": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "attractions",
        "events",
        "both"
      ]
    },
    "counties": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::county.county",
      "mappedBy": "highlights"
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "highlights"
    },
    "attractions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::attraction.attraction",
      "mappedBy": "highlights"
    },
    "destinations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::destination.destination",
      "mappedBy": "highlights"
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "uid",
      "targetField": "title"
    },
    "locations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::location.location",
      "mappedBy": "highlights"
    },
    "monuments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::monument.monument",
      "mappedBy": "highlights"
    },
    "regions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::region.region",
      "mappedBy": "highlights"
    },
    "displayTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "unique": true,
      "default": "title [category]",
      "required": true
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "highlights"
    }
  }
}
